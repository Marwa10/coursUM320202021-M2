<!-- Table covid_air_quality -->
<rdfs:Class rdf:about="/rdfvocabulary#covid_air_quality">
  <rdfs:label xml:lang="en">covid Air quality</rdfs:label>
  <rdfs:comment xml:lang="fr">Un concept pour représenter la qualité de l'aire
